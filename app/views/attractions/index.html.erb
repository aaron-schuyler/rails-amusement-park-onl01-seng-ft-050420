
<%= link_to "New Attraction", new_attraction_path %>
<table>
  <thead>
    <th>Name</th><th>Tickets</th><th>Details</th>
  </thead>
  <% @attractions.each do |attraction| %>
    <tr>
      <td><%= link_to "Show #{attraction.name}", attraction_path(attraction) %></td><td><%= attraction.tickets %></td><td><%= link_to "Go on #{attraction.name}", attraction_path(attraction) if !current_user.admin %></td>
    </tr>
  <% end %>
</table>
